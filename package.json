{
  "name": "dollop-web-service",
  "version": "0.0.1",
  "description": "Dollop Web Service",
  "author": " Gabriel Scaranello <gabrieljscaranello@gmail.com>",
  "private": false,
  "license": "MIT",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"**/*.{js,ts,vue,md,json}\" --ignore-path .gitignore",
    "lint": "eslint --ext .js,.ts, ./",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "test": "vitest run --passWithNoTests",
    "test:watch": "vitest --passWithNoTests --watch",
    "test:ci": "npm test -- --coverage",
    "test:ui": "npm run test:watch -- --ui",
    "test:debug": "npm test -- --inspect-brk --inspect --logHeapUsage --threads=false",
    "test:e2e": "npm test -- --config ./vitest.config.e2e.ts",
    "typeorm": "typeorm-ts-node-commonjs",
    "typeorm:extension": "typeorm-extension",
    "prepare": "husky install"
  },
  "dependencies": {
    "@nestjs/common": "^9.0.0",
    "@nestjs/config": "^2.3.1",
    "@nestjs/core": "^9.0.0",
    "@nestjs/jwt": "^10.0.3",
    "@nestjs/platform-express": "^9.0.0",
    "@nestjs/typeorm": "^9.0.1",
    "bcrypt": "^5.1.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "pg": "^8.10.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1",
    "typeorm": "^0.3.15",
    "typeorm-extension": "^3.0.1"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.0.2",
    "@nestjs/cli": "^9.4.2",
    "@nestjs/schematics": "^9.0.0",
    "@nestjs/testing": "^9.0.0",
    "@swc/core": "^1.3.56",
    "@types/bcrypt": "^5.0.0",
    "@types/express": "^4.17.13",
    "@types/node": "18.16.3",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.59.1",
    "@typescript-eslint/parser": "^5.59.1",
    "@vitest/coverage-istanbul": "^0.31.0",
    "@vitest/ui": "^0.31.0",
    "eslint": "^8.39.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-config-standard-with-typescript": "^34.0.1",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-n": "^15.7.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-vue": "^9.11.0",
    "git-commit-msg-linter": "^4.10.0",
    "husky": "^8.0.3",
    "identity-obj-proxy": "^3.0.0",
    "lint-staged": "^13.2.2",
    "prettier": "^2.8.8",
    "prettier-config-standard": "^5.0.0",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "ts-loader": "^9.4.2",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "4.2.0",
    "typescript": "^5.0.4",
    "unplugin-swc": "^1.3.2",
    "vitest": "^0.31.0"
  }
}
